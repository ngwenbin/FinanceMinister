{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "moduleResolution": "node",
    "noEmit": true,
    "esModuleInterop": true,
    "strict": true,
    "jsx": "react-native",
    "lib": ["dom", "esnext"],
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@*": ["src/*"],
      "@assets": ["src/assets/*"],
      "@components": ["src/components/*"],
      "@constants/*": ["src/constants/*"],
      "@hooks": ["src/hooks/*"],
      "@navigation": ["src/navigation/*"],
      "@screens": ["src/screens/*"],
      "@utils/*": ["src/utils/*"],
      "@typings/*": ["src/typings/*"],
      "@providers/*": ["src/providers/*"]
    }
  },
  "include": ["src/**/*", "src/App.tsx", "ReamApp.ts", "src/declaration.d.ts"],
  "exclude": ["node_modules"]
}
